{
  "name": "CITB Rec",
  "version": "1.0",
  "description": "CITB Rec Utility",
  "manifest_version": 2,
  "key": "",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "oauth2": {
    "client_id": "19686602327-poevejt2q5aqpctosm08t6kueaun1rrk.apps.googleusercontent.com",
